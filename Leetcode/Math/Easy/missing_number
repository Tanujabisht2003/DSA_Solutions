<!-- https://leetcode.com/problems/missing-number/description/?envType=problem-list-v2&envId=math&status=SOLVED&difficulty=EASY -->

class Solution {
    public int missingNumber(int[] nums) {
        Arrays.sort(nums);
        int j=0;
        int n= nums.length;
        for(int i=0;i<n;i++){
            if(nums[i] == j){
                j++;
                continue;
            }
        }
        return j;
    }
}