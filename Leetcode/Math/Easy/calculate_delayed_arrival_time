<!-- https://leetcode.com/problems/calculate-delayed-arrival-time/description/?envType=problem-list-v2&envId=math&status=SOLVED&difficulty=EASY -->

class Solution {
    public int findDelayedArrivalTime(int a, int d) {
        if(a+d >= 24){
            return (a+d)-24;
        }else{
            return a+d;
        }
    }
}