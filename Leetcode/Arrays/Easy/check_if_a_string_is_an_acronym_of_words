// https://leetcode.com/problems/check-if-a-string-is-an-acronym-of-words/submissions/1029480686/?envType=problem-list-v2&envId=array&status=SOLVED&difficulty=EASY

class Solution {
    public boolean isAcronym(List<String> words, String s){
        String m = "";
        for(int i=0;i<words.size();i++){
            String temp = words.get(i);
            m = m + temp.charAt(0);
        }
            if(m.equals(s)){
                return true;
            }
        return false;
        }
    
}