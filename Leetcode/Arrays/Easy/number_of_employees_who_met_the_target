<!-- https://leetcode.com/problems/number-of-employees-who-met-the-target/description/?envType=problem-list-v2&envId=array&status=SOLVED&difficulty=EASY -->


class Solution {
    public int numberOfEmployeesWhoMetTarget(int[] hours, int target) {
        int sum = 0;
        int n = hours.length;
        for(int i=0;i<n;i++){
            if(hours[i] >= target){
                sum++;
            }
        }
        return sum;
    }
}