<!-- https://leetcode.com/problems/search-a-2d-matrix/description/?envType=problem-list-v2&envId=array&status=SOLVED&difficulty=MEDIUM -->

class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        for(int i=0;i<matrix.length;i++){
            for(int j=0;j<matrix[i].length;j++){
                if(target == matrix[i][j]){
                    return true;
                }
            }
        }
        return false;
    }
}