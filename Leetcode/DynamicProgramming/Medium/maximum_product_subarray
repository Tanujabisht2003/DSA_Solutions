<!-- https://leetcode.com/problems/maximum-product-subarray/?envType=problem-list-v2&envId=dynamic-programming&status=SOLVED&difficulty=MEDIUM -->

class Solution {
    public int maxProduct(int[] nums) {
        int max = 0;
        double product ;
        if(nums.length == 1){
            return nums[0];
        }
        for(int i=0;i<nums.length;i++){
            product = 1;
            for(int j=i;j<nums.length;j++){
                product *= nums[j];
                max = Math.max(max, (int) product);
            }
        }
        // int max1 = (int) max;
        return max;
    }
}
