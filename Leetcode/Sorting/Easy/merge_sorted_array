<!-- https://leetcode.com/problems/merge-sorted-array/description/?envType=problem-list-v2&envId=sorting&status=SOLVED&difficulty=EASY -->

class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        for(int i=0;i<nums2.length;i++){
            nums1[m+i] = nums2[i];
        }
        Arrays.sort(nums1);
    }
}